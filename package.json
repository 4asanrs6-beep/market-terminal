{
  "name": "market-terminal",
  "version": "1.0.0",
  "description": "Bloomberg-style stock market terminal",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -n vite,electron \"vite\" \"node scripts/wait-and-electron.js\"",
    "build:electron": "node scripts/build-electron.js",
    "build": "vite build && node scripts/build-electron.js",
    "dist": "npm run build && node scripts/package.js"
  },
  "dependencies": {
    "lightweight-charts": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yahoo-finance2": "^2.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^9.2.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "esbuild": "^0.19.12",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
